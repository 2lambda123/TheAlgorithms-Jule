fn test_kruskal() {
    graph := Graph{
        vertices: 4,
        edges: [
            Edge{0, 1, 10},
            Edge{0, 2, 6},
            Edge{0, 3, 5},
            Edge{1, 3, 15},
            Edge{2, 3, 4}
        ]
    }
    kruskal_mst(graph)
}

fn test_floyd_warshall() {
    graph := [][]i32{
        {0, 5, 99999, 10},
        {99999, 0, 3, 99999},
        {99999, 99999, 0, 1},
        {99999, 99999, 99999, 0}
    }
    floyd_warshall(graph)
}

