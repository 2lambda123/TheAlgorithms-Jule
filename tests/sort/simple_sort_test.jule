use tests::{TestStack}
use sort::{simple_sort}

fn simple_sort_tests(mut &tests: TestStack) {
    let mut unsorted = [9, 35, -0, 0, 98, 8935, -85, -9835, 64, 89]
    let sorted = [-9835, -85, -0, 0, 9, 35, 64, 89, 98, 8935]

    tests.add("[sort] simple_sort", fn(): bool {
        ret simple_sort(unsorted) == sorted
    })
}
