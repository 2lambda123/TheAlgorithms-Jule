use tests::{TestStack}
use sort::{quick_sort}

fn quick_sort_tests(mut &tests: TestStack) {
    let mut unsorted = [9, 35, -0, 0, 98, 8935, -85, -9835, 64, 89]
    let sorted = [-9835, -85, -0, 0, 9, 35, 64, 89, 98, 8935]

    tests.add("quick_sort", fn(): bool {
        ret quick_sort(unsorted) == sorted
    })
}
