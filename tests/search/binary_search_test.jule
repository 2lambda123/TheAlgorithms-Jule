use tests::{TestStack}
use search::{binary_search}

fn binary_search_tests(mut &tests: TestStack) {
    let s = [-9835, -85, -0, 0, 9, 35, 64, 89, 98, 8935]

    tests.add("[search] binary_search:(1)", fn(): bool {
        ret binary_search(s, 90) == -1
    })

    tests.add("[search] binary_search:(2)", fn(): bool {
        ret binary_search(s, -85) == 1
    })

    tests.add("[search] binary_search:(3)", fn(): bool {
        ret binary_search(s, 89) == 7
    })
}
