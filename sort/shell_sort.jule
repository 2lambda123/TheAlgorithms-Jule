pub fn ShellSort(mut s: []int): []int {
    let mut d = int(s.len / 2)
    for d > 0; d /= 2 {
        let mut i = d
        for i < s.len; i++ {
            let mut j = i
            for j >= d && s[j-d] > s[j]; j -= d {
                s[j], s[j-d] = s[j-d], s[j]
            }
        }
    }
    ret s
}
